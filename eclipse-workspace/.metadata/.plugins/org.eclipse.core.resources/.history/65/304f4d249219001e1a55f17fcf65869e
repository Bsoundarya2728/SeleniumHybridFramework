package practise_Java;

class increment{
	int count;
	public void Totalcount() {
		count++;
	}
}

public class ThreadConcepts {
	public static void main(String[] args) {
		
		increment inc = new increment();
		
		Thread t1= new Thread(new Runnable) 
		{	
			public void run()
			{
				for(int i=1;i<1000;i++)
				{
					inc.Totalcount();
				}
			}
		};	
		}
	

}
